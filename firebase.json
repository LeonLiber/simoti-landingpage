{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/tagLogger",
        "function": "tagLogger"
      },
      {
        "source": "/getSnippet",
        "function": "getSnippet"
      }
    ],
    "headers": [
      {
        "source": "**/simoti*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
